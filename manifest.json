{
  "manifest_version": 3,
  "name": "Quick URL Shortener",
  "version": "1.0",
  "description": "Shortens the current tab URL and copies it to clipboard.",
  "permissions": ["tabs", "activeTab", "scripting", "notifications", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_icon": {
      "16": "assets/Shapes/W.svg",
      "32": "assets/Shapes/W.svg",
      "48": "assets/Shapes/W.svg",
      "128": "assets/Shapes/W.svg"
    }
  },
  "icons": {
    "16": "assets/Shapes/W.svg",
    "32": "assets/Shapes/W.svg",
    "48": "assets/Shapes/W.svg",
    "128": "assets/Shapes/W.svg"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "open-popup": {
      "suggested_key": {
        "default": "Ctrl+Shift+H"
      },
      "description": "Open history popup"
    }
  }
}